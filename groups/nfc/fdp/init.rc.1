on boot
    # Load NFC I2C driver
    insmod ${ro.boot.moduleslocation}/fdp_nfc.ko

on post-fs-data
    # create config NFC NVM folder
    mkdir /config/nfc 0770 nfc system
    restorecon_recursive /config/nfc
    start nfc_rest_nvm

service nfc_rest_nvm /system/bin/sh /system/bin/nfc_intel_restore.sh
    class main
    user nfc
    group system nfc
    disabled
    oneshot

