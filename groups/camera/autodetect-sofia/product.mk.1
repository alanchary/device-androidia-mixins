ifeq (${TARGET_BOARD_PLATFORM},sofia_lte)
PRODUCT_COPY_FILES += \
    $(LOCAL_PATH)/camera/camera3_profiles.xml:$(HAL_FUSE_PREFIX_DEFAULT)/camera3_profiles_sofialte.xml
endif
