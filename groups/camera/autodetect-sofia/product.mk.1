# Copy specific camera3_profiles.xml files into hald default fuse folder. The
# correct files will be fused into /system/rt/hal_fuse upon device detection.

PRODUCT_COPY_FILES += \
	vendor/intel/camera/camera3hal/config/sofia3gr/Sf3gr_mrd6_p1/camera3_profiles.xml:$(HAL_FUSE_PREFIX_DEFAULT)/camera3_profiles_sofia3gr_mrd6.xml \
	vendor/intel/camera/camera3hal/config/sofia3gr/Sf3gr_oars7/camera3_profiles.xml:$(HAL_FUSE_PREFIX_DEFAULT)/camera3_profiles_sofia3gr_oars7.xml
