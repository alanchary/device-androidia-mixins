on boot
     insmod ${ro.boot.moduleslocation}/snd-hda-core.ko
     insmod ${ro.boot.moduleslocation}/snd-hda-ext-core.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-sst-acpi.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-sst-dsp.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-sst-ipc.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-skl-ipc.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-skl.ko
     insmod ${ro.boot.moduleslocation}/arizona-micsupp.ko
     insmod ${ro.boot.moduleslocation}/arizona-ldo1.ko
     insmod ${ro.boot.moduleslocation}/arizona-i2c.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-arizona.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-wm-adsp.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-wm5110.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-wm8998.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-dmic.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-hdac-hdmi.ko
     insmod ${ro.boot.moduleslocation}/snd-soc-bxt_mrgfld_dpcm_florida.ko

{{#wov}}
# Set the WakeOnVoice properties
on boot
    setprop audio.wov.card wm8998audio
    setprop audio.wov.device 0
    setprop audio.wov.switch "hwd_in Switch"
    setprop audio.wov.params "hwd_in hwd 0 mdl params"
    setprop audio.wov.notif "hwd_in hwd 0 notif params"
{{/wov}}

{{#aware}}
on boot
    setprop audio.aware.mixer "aware_out classifier 0 mdl params"
    setprop audio.aware.card   wm8998audio
{{/aware}}
