on fs
    mkdir /dev/pstore 0775 root system
    mount pstore pstore /dev/pstore

on post-fs-data
   mkdir /data/dontpanic 0775 root log

service pstore-clean /system/vendor/bin/pstore-clean
    user system
    group root system log
    class late_start
    oneshot

on property:init.svc.pstore-clean=stopped
    umount /dev/pstore
    rmdir /dev/pstore
